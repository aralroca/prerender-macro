{
  "name": "prerender-macro",
  "version": "0.0.5",
  "module": "package/index.ts",
  "type": "module",
  "devDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@types/bun": "1.0.10",
    "@types/react": "18.2.69",
    "@types/react-dom": "18.2.22",
    "brisa": "0.0.34",
    "preact": "10.20.1"
  },
  "dependencies": {
    "typescript": "5.4.3"
  },
  "scripts": {
    "test": "bun run test:brisa && bun run test:react && bun run test:preact",
    "test:brisa": "cd examples-and-tests/brisa && bun test && cd ../..",
    "test:react": "cd examples-and-tests/react && bun test && cd ../..",
    "test:preact": "cd examples-and-tests/preact && bun test && cd ../.."
  },
  "workspaces": [
    "package",
    "examples-and-tests/*"
  ],
  "files": [
    "package"
  ],
  "exports": {
    ".": {
      "import": "./package/index.ts",
      "require": "./package/index.ts"
    },
    "./prerender": {
      "import": "./package/prerender.tsx",
      "require": "./package/prerender.tsx"
    }
  }
}
